{
  "name": "elasticsidebar",
  "version": "0.0.1",
  "description": "Elastic Sidebar Menu implementation.",
  "main": "build.js",
  "scripts": {
    "lint": "npm run lint-js && npm run lint-sass",
    "lint-js": "./node_modules/.bin/eslint source/**/*.js tasks/**/*.js",
    "lint-js-fix": "./node_modules/.bin/eslint source/**/*.js tasks/**/*.js --fix",
    "lint-sass": "./node_modules/.bin/sass-lint -c .sass-lint.yml --verbose --max-warnings 10",
    "test": "npm run lint",
    "precommit": "gulp build && git add dist/*.* docs/*.js docs/*.css",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FranciscoKnebel/ElasticSidebar.git"
  },
  "keywords": [
    "sidebar",
    "menu",
    "elastic"
  ],
  "author": "Francisco Knebel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FranciscoKnebel/ElasticSidebar/issues"
  },
  "homepage": "https://github.com/FranciscoKnebel/ElasticSidebar#readme",
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-gzip": "^1.4.0",
    "gulp-jsbeautifier": "^2.0.4",
    "gulp-load-plugins": "^1.4.0",
    "gulp-optimize-js": "^1.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sass-glob": "^1.0.8",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "run-sequence": "^1.2.2"
  },
  "devDependencies": {
    "eslint": "^3.8.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "husky": "^0.11.9",
    "sass-lint": "^1.10.2"
  }
}
